{
  "name": "GestureHandlerExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "bundle_testOnly": "react-native bundle --entry-file='./index.ios.js' --bundle-output='./ios/GestureHandler/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "postinstall": "rm -rf node_modules/react-native-gesture-handler/{.git,node_modules,Example,AndroidNativeExample}"
  },
  "dependencies": {
    "react": "16.5.1",
    "react-native": "0.57.1",
    "react-native-gesture-handler": "file:..",
    "react-native-vector-icons": "5.0.0",
    "react-navigation": "2.17.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react-native": "^5.0.2",
    "detox": "^9.0.4",
    "jest": "23.6.0",
    "jest-react-native": "^18.0.0",
    "metro-react-native-babel-preset": "0.45.2",
    "react-test-renderer": "^16.5.2"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "test/e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "./ios/build/Build/Products/Debug-iphonesimulator/GestureHandler.app",
        "build": "xcodebuild -project ./ios/GestureHandler.xcodeproj -scheme GestureHandler -configuration Debug -sdk iphonesimulator -derivedDataPath ./ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  }
}
